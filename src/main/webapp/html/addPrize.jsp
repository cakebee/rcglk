	<link rel="stylesheet"
		href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.12.4/css/bootstrap-select.min.css">
	<script
		src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.12.4/js/bootstrap-select.min.js">
	</script>
</head>
<body style="background-color: #E5E8E8;">
	<div class="container-fluid">
		<div class="row">
			<div class="col-sm-2 logofont">
				<p>ＵＥＳＴＣ</p>
			</div>
			<div class="navcss">
        <div class="col-sm-6">
          <ul class="mynav">
            <li class="mynav"><a class="mynav" href="">我的信息</a></li>
            <li class="mynav"><a class="mynav" href="./404.jsp">查询信息</a></li>
            <li class="mynav"><a class="mynav" href="./404.jsp">生成简历</a></li>
          </ul>
        </div>
        <div class="col-sm-2" style="text-align: right;">${sessionScope.username}</div>
        <div class="col-sm-2"><a href="" class="mynav">退出登录</a></div>
      </div>
		</div>
		<div class="row">
			<div class="col-sm-2 sidebarstyle">
				<ul class="mysidebar">
					<li class="mysidebar"><a class="mysidebar" href="studentInformation.jsp">基本信息</a></li>
					<li class="mysidebar"><a class="mysidebar" href="#details">详细信息</a></li>
					<li class="mysidebar" id="charchange2"><a class="mysidebar"
						onclick="opennav2()" style="cursor: pointer;">获奖情况&dtrif;</a></li>
					<li class="a" id="mySidebar3"><a class="mysidebar"
						href="addPrize.jsp">添加获奖信息</a></li>
					<li class="a" id="mySidebar4"><a class="mysidebar"
						href="viewPrize.jsp">查看获奖信息</a></li>
					<li class="mysidebar" id="charchange1"><a class="mysidebar"
						onclick="opennav1()" style="cursor: pointer;">活动经历&dtrif;</a></li>
					<li class="a" id="mySidebar1"><a class="mysidebar"
						href="addExperience.jsp">添加活动经历</a></li>
					<li class="a" id="mySidebar2"><a class="mysidebar"
						href="viewActivity.jsp">查看活动经历</a></li>
					<li class="mysidebar"><a class="mysidebar" href="status.jsp">审核进度</a></li>
				</ul>
			</div>
			<div class="col-sm-10">
				<div class="formbox">
					<div class="progress">
						<div class="progress-bar" role="progressbar" aria-valuenow="60"
							aria-valuemin="0" aria-valuemax="100" style="width: 40%;">40%</div>
					</div>
					<div id="addname">
						<div ng-app="prize_class" ng-controller="prize_classCtrl">
							<label for="prize_class1">获奖类别*:</label> <br> <select
								ng-model="selectclass1" ng-mouseleave="class2menu()" class=""
								id="prize_class1">
								<option value="0">请选择...</option>
								<option ng-repeat="x in class1">{{x}}</option>
							</select> <br> <label for="prize_class">所获奖项*:</label> <br> <select
								ng-model="selectclass2" class="" name="prize_class"
								id="prize_class">
								<option value="0">请选择...</option>
								<option ng-repeat="y in class2">{{y}}</option>
							</select> <br> <label for="prizeLevel">奖项级别*:</label> <br> <select
								class="selectpicker" id="prizeLevel">
								<option value="0">请选择...</option>
								<option value="国际级">国际级</option>
								<option value="国家级">国家级</option>
								<option value="省级">省级</option>
								<option value="市级">市级</option>
								<option value="校级">校级</option>
								<option value="院级">院级</option>
							</select> <br> <label for="prize_name">获奖名称*:</label>
							<div class="input-group">
								<span class="input-group-addon"><i class="icon-glass"></i></span>
								<input type="text" class="form-control" placeholder="请输入获奖名称..."
									id="prizeName">
							</div>
							<label for="prize_date">获奖时间*:</label>
							<div class="input-group">
								<span class="input-group-addon"><i class="icon-list-alt"></i></span>
								<input type="date" class="form-control" id="prizeDate">
							</div>
							<label for="prizeFile">证明文件*:</label>
							<input type='file' id='prizeFile'>(请上传小于200kb的图片文件)
							<br>
							<label for="prizeIntro">说明:</label>
							<br>
							<textarea id='prizeIntro'
								style='width: 250px; height: 100px; resize: none;'></textarea>
							<br>
							<button onclick="addPrize()" class="btn btn-success">提交</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script>
	function addPrize(){
		var over = false;
		var prizeClass=$('#prize_class').val();
		var prizeLevel=$('#prizeLevel').val();
		var prizeName=$('#prizeName').val();
		var prizeDate=$('#prizeDate').val();
		var prizeFile= document.getElementById('prizeFile').files[0];
		var prizeIntro=$('#prizeIntro').val();
		var formdata=new FormData();
		formdata.append('file',prizeFile);
		// 获取当前日期
		var date = new Date();
		// 获取当前月份
		var nowMonth = date.getMonth() + 1;
		// 获取当前是几号
		var strDate = date.getDate();
		// 添加分隔符“-”
		var seperator = "-";
		// 对月份进行处理，1-9月在前面添加一个“0”
		if (nowMonth >= 1 && nowMonth <= 9) {
		   nowMonth = "0" + nowMonth;
		}
		// 对月份进行处理，1-9号在前面添加一个“0”
		if (strDate >= 0 && strDate <= 9) {
		   strDate = "0" + strDate;
		}
		// 最后拼接字符串，得到一个格式为(yyyy-MM-dd)的日期
		var nowDate = date.getFullYear() + seperator + nowMonth + seperator + strDate;
		console.log(prizeFile.size)
		if(prizeDate>nowDate){
			alert("请填写正确日期...");
			return;
		}
		if(prizeLevel == "0"||prizeName == ""||prizeDate == ""||prizeFile == null||prizeDate == ""||prizeClass == "0"){
			alert("请完整填写信息...")
			return;
		}
		if(prizeFile.size > 204800){
			alert("文件大小超出限制，请压缩后重试");
			return;
		}
		if(prizeFile.type.split('/')[0] != 'image'){
			alert("请上传图片类型的文件");
			return;
		}
		console.log(prizeFile)
		var studentsprize={
		  "stuId": $.cookie('user'),
		  "stuName":"王志远",
		  "prizeName":prizeName,
		  "prizeDate":prizeDate,
		  "prizeClass":prizeClass,
		  "prizeLevel":prizeLevel,
		  "prizeIntro":prizeIntro,
		  "status":"0"
		};
		$.ajax({
			url: "/rcglk/file",
			type: 'POST',
			data: formdata,
			async: false,
			processData: false,
			contentType: false,
			success: function(){
				console.log("file success");
			},
			error: function(){
				alert("提交失败，请稍后再试");
				over = true;
			}
		});
		if(over == true){
			return;
		}
		$.ajax({
		    url : "../Studentsprize",
			type : "POST",
			data: studentsprize,
			async: false,
			// 向后端发送的数据
			success:function(data) {
				alert("提交成功");
			},
			error:function(err){
				console.log(err)
				alert("提交失败，请稍后再试");
			}
		// 处理后端返回的数据
		});
	}
	</script>
</body>
</html>